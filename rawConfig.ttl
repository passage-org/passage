PREFIX :        <#>
PREFIX fuseki:  <http://jena.apache.org/fuseki#>
PREFIX rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ja:      <http://jena.hpl.hp.com/2005/11/Assembler#>
PREFIX tdb2:    <http://jena.apache.org/2016/tdb#>
PREFIX psg:     <http://fr.gdd.passage/engine-vocabulary#>

## For more information about Fuseki configurations, take a look at:
## <https://jena.apache.org/documentation/fuseki2/fuseki-configuration.html>

## SERVER

[] rdf:type fuseki:Server ;
   fuseki:services (
     :service_fedshop200
     :service_fedshop200_raw
   )
   .

## SERVICES

:service_fedshop200 rdf:type fuseki:Service ;
    fuseki:name "fedshop200p" ;
    fuseki:endpoint [
        fuseki:operation psg:query_w_args ;
        fuseki:name "passage" ;
    ];
    fuseki:dataset :dataset_fedshop200 ;
    .

:service_fedshop200_raw rdf:type fuseki:Service ;
    fuseki:name "fedshop200" ;
    fuseki:endpoint [
        fuseki:operation psg:query_w_args ;
        fuseki:name "raw" ;
    ];
    fuseki:dataset :dataset_fedshop200_raw ;
    .

## DATASETS

:dataset_fedshop200 rdf:type psg:DatasetBlazegraph ;
    psg:location           "/Users/boisteau-desdevises-e/Documents/datasets/fedshop200.properties" ;
    psg:timeout            10000 ;
    psg:max_results        10 ;
    psg:engine             psg:PassageEngine ;
    .

:dataset_fedshop200_raw rdf:type psg:DatasetBlazegraph ;
    psg:location           "/Users/boisteau-desdevises-e/Documents/datasets/fedshop200.properties" ;
    psg:timeout            10000 ;
    psg:max_results        10 ;
    psg:engine             psg:RawEngine ; ## TODO change this engine for RAW
    .